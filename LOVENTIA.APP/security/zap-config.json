{
  "configuration": {
    "target": "http://localhost:3000",
    "context": {
      "include": [
        "http://localhost:3000/.*"
      ],
      "exclude": []
    },
    "authentication": {
      "method": "manual"
    },
    "scanPolicyName": "Default Policy",
    "scanners": {
      "xssActive": true,
      "xssPassive": true,
      "csrfTokens": [
        {
          "name": "_csrf",
          "location": "body"
        }
      ]
    }
  }
}
