    "date-fns": "^2.30.0",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.1.0",
    "i18next-http-backend": "^3.0.2",
    "leaflet": "^1.9.4",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.59.0",
