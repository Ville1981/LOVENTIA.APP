{
  "SLIs": [
    {
      "name": "api_error_rate",
      "namespace": "Application/Metrics",
      "metricName": "ErrorRate",
      "statistic": "Average",
      "threshold": 0.01,
      "evaluationPeriods": 5,
      "periodSeconds": 60
    },
    {
      "name": "api_latency_p95",
      "namespace": "Application/Metrics",
      "metricName": "Latency",
      "statistic": "p95",
      "threshold": 300,
      "evaluationPeriods": 5,
      "periodSeconds": 60
    }
  ],
  "SLOs": [
    {
      "sliName": "api_error_rate",
      "target": 0.99,
      "windowDays": 30
    },
    {
      "sliName": "api_latency_p95",
      "target": 0.95,
      "windowDays": 30
    }
  ]
}